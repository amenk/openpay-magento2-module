<?xml version="1.0"?>

<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <!-- Credentials -->
    <route url="/V1/openpay/credentials" method="GET">
        <service class="Degaray\Openpay\Api\CredentialsRepositoryInterface" method="get"/>
        <resources>
            <resource ref="self"/>
        </resources>
    </route>
</routes>
{
  "name": "degaray/openpay",
  "description": "OpenPay module for Magento2, a payment gateway in Mexico",
  "require": {
    "php": "~5.5.0|~5.6.0",
    "magento/magento-composer-installer": "*",
    "degaray/openpay-php": "master-dev"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/degaray/openpay-php.git"
    }
  ],
  "type": "magento2-module",
  "version": "0.1.1",
  "extra": {
    "map": [
      [
        "*",
        "Degaray/Openpay"
      ]
    ]
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Degaray\\Openpay\\": ""
    }
  }
}
